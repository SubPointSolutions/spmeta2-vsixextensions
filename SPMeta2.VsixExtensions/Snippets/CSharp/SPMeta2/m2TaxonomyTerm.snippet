<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">

  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>Taxonony Term Definition</Title>
      <Author>SubPoint Solutions</Author>
      <Description>Creates a new taxonomy term definition.</Description>
      <HelpUrl>
      </HelpUrl>
      <Shortcut>m2TaxonomyTerm</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>SPMeta2.Definitions</Namespace>
        </Import>
        <Import>
          <Namespace>SPMeta2.Enumerations</Namespace>
        </Import>
        <Import>
          <Namespace>SPMeta2.Standard.Definitions.Taxonomy</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal Editable="true">
          <ID>name</ID>
          <Default>NewTaxonomyTerm</Default>
        </Literal>
        <Literal Editable="true">
          <ID>termName</ID>
          <Default>NewTaxonomyTerm</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp" Delimiter="$">
        <![CDATA[public static TaxonomyTermDefinition $name$ = new TaxonomyTermDefinition
        {
            Name = "$termName$"
        };
	    ]]>
      </Code>
    </Snippet>
  </CodeSnippet>

</CodeSnippets>