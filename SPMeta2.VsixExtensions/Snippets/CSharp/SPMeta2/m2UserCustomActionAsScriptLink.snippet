<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">

  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>Custom User Action as ScriptLink</Title>
      <Author>SubPoint Solutions</Author>
      <Description>Creates a new custom user action.</Description>
      <HelpUrl>
      </HelpUrl>
      <Shortcut>m2UserCustomActionAsScriptLink</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>System.Collections.ObjectModel</Namespace>
        </Import>
        <Import>
          <Namespace>SPMeta2.Definitions</Namespace>
        </Import>
        <Import>
          <Namespace>SPMeta2.Enumerations</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal Editable="true">
          <ID>name</ID>
          <Default>NewCustomUserAction</Default>
        </Literal>
        <Literal Editable="true">
          <ID>userCustomActionName</ID>
          <Default>NewCustomUserActionname</Default>
        </Literal>
        <Literal Editable="true">
          <ID>scriptSrc</ID>
          <Default>ScriptLinkUrl</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp" Delimiter="$">
        <![CDATA[public static UserCustomActionDefinition $name$ = new UserCustomActionDefinition
        {
            Name = "$userCustomActionName$",
            Location = "ScriptLink",
            ScriptSrc = "$scriptSrc$",
            Sequence = 15000
        };
	    ]]>
      </Code>
    </Snippet>
  </CodeSnippet>

</CodeSnippets>